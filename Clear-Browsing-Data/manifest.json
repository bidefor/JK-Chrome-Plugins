{
    "manifest_version": 2,
    "name": "Clear Browsing Data",
    "version": "0.0.4",
    "description": "one key to clear chrome browsing data, such as AppCache, LocalStorage, Cookie and so on.",
    "browser_action": {
        "default_title": "Clear Browsing Data",
        "default_icon": "assets/images/logos/19x19.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "assets/images/logos/19x19.png",
        "48": "assets/images/logos/48x48.png",
        "128": "assets/images/logos/128x128.png"
    },
    "permissions": [
        "browsingData",
        "contextMenus"
    ],
    "background": {
        "scripts": ["assets/js/background.js"]
    }
}
